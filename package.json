{
  "dependencies": {
    "@expo-google-fonts/roboto": "^0.1.0",
    "@expo-google-fonts/ubuntu": "^0.1.0",
    "@react-firebase/database": "^0.3.11",
    "@react-native-community/checkbox": "^0.5.5",
    "@react-native-community/datetimepicker": "^3.0.4",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.10.6",
    "@react-navigation/native": "^5.8.2",
    "@react-navigation/stack": "^5.11.1",
    "expo": "^39.0.3",
    "expo-application": "^2.3.0",
    "expo-auth-session": "~2.0.0",
    "expo-constants": "~9.2.0",
    "expo-font": "~8.3.0",
    "expo-google-app-auth": "^8.1.3",
    "firebase": "^8.0.0",
    "react": "^17.0.1",
    "react-dom": "~17.0.1",
    "react-native": "^0.63.3",
    "react-native-appearance": "~0.3.4",
    "react-native-dialog": "^6.1.2",
    "react-native-gesture-handler": "~1.8.0",
    "react-native-reanimated": "~1.13.1",
    "react-native-safe-area-context": "3.1.8",
    "react-native-screens": "~2.12.0",
    "react-native-web": "0.14.7"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.15",
    "@types/react": "~16.9.55",
    "@types/react-dom": "~16.9.9",
    "@types/react-native": "~0.63.30",
    "@types/react-native-dialog": "^5.6.2",
    "@types/react-test-renderer": "^16.9.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jest-expo": "^39.0.0",
    "jest-react-hooks-shallow": "^1.4.1",
    "make-coverage-badge": "^1.2.0",
    "react-test-renderer": "^17.0.1",
    "typescript": "~4.0.5"
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo web",
    "eject": "expo eject",
    "deploy": "expo publish",
    "update:packages": "npm audit fix -f && npm-check-updates -u && npm update && npm install",
    "test": "jest",
    "test:coverage": "jest --verbose --coverage --watchAll",
    "test:badges": "npm run test:coverage && make-coverage-badge --output-path ./badges/coverage.svg"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "setupFiles": [
      "<rootDir>jest/setupTest.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>jest/setupFilesAfterEnv.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary",
      "json-summary"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/styles.tsx",
      "!**/src/styles/**",
      "!**/src/types/**",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "verbose": true
  },
  "private": true
}